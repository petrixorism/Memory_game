            android:tag="close"
            app:contentPadding="16dp"
            app:layout_constraintBottom_toTopOf="@id/pic10"
            app:layout_constraintEnd_toStartOf="@+id/pic8"
            app:layout_constraintHeight_percent=".22"
            app:layout_constraintHorizontal_chainStyle="spread"
